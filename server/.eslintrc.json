{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-unused-vars": 0,
        "consistent-return": 0,
        "no-undef": 0,
        "no-unused-expressions": 0,
        "no-underscore-dangle": 0
    }
}
